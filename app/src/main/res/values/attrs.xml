<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Theme">
        <attr name="themeColor" format="reference"/>
        <attr name="formatColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="TypeFaceTextView">
        <attr name="typeface">
            <enum name="BebasNeueRegular" value="0"/>
            <enum name="BebasNeueBook" value="1"/>
            <enum name="Roboto_Medium" value="2"/>
            <enum name="Roboto_Light" value="3"/>
            <enum name="Roboto_Regular" value="4"/>
            <enum name="SanFranciscoDisplay_Medium" value="5"/>
            <enum name="MyriadPro" value="6"/>
        </attr>
    </declare-styleable>

</resources>